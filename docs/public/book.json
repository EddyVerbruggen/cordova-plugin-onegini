{
  "gitbook": "2.x.x",
  "title": "Onegini Cordova plugin",
  "description": "The official documentation of the Onegini Cordova plugin.",
  "plugins": [
    "expandable-chapters",
    "toc",
    "disqus",
    "anker-enable",
    "prism",
    "-highlight",
    "websequencediagrams",
    "onegini-styling",
    "-fontsettings"
  ],
  "pluginsConfig": {
    "disqus": {
      "shortName": "onegini-docs"
    }
