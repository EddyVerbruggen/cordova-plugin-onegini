  "styles": {
    "pdf": "styles/pdf.css",
    "epub": "styles/pdf.css"
  },
  "links": {
    "gitbook": false,
    "sharing": {
      "google": false,
      "facebook": false,
      "twitter": false,
      "weibo": false,
      "all": false
    }
  }
}
